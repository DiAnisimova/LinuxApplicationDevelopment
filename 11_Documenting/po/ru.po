#: src/guess.c:84
#, c-format
msgid "Usage: ./program_name [OPTION]\n"
msgstr "Использование: ./program_name [АРГУМЕНТ]\n"

#: src/guess.c:85
#, c-format
msgid "Options:\n"
msgstr "Аргументы:\n"

#: src/guess.c:86
#, c-format
msgid "\t-r\tPlay the game using Roman numerals instead of numbers.\n"
msgstr "\t-r\tИграть с использованием римских цифр.\n"

#: src/guess.c:87
#, c-format
msgid "\t--help\tDisplay this help message and exit.\n"
msgstr "\t--help\tВывести это сообщение и выйти.\n"

#: src/guess.c:111
#, c-format
msgid "Think of a number between 1 and 100.\n"
msgstr "Загадайте число от 1 до 100.\n"

#: src/guess.c:118
#, c-format
msgid "Is your number greater than %s? (yes/no): "
msgstr "Ваше число больше %s? (да/нет): "

#: src/guess.c:120
#, c-format
msgid "Is your number greater than %d? (yes/no): "
msgstr "Ваше число больше %d? (да/нет): "

#: src/guess.c:124
#, c-format
msgid "Error reading input.\n"
msgstr "Ошибка чтения ввода.\n"

#: src/guess.c:128
msgid "yes"
msgstr "да"

#: src/guess.c:130
msgid "no"
msgstr "нет"

#: src/guess.c:133
#, c-format
msgid "Please answer 'yes' or 'no'.\n"
msgstr "Пожалуйста, ответьте 'да' или 'нет'.\n"

#: src/guess.c:140
#, c-format
msgid "Your number is %s!\n"
msgstr "Ваше число - %s!\n"

#: src/guess.c:142
#, c-format
msgid "Your number is %d!\n"
msgstr "Ваше число - %d!\n"
